package com.recruit.entity;

public class FindWorks {

    private int riId;
    private int cid;
    private String job;
    private String annual_salary;
    private String location;
    private String experience;
    private String education;
    private String job_info;
    private String release_time;
    private String companyName;
    private String industry;
    private String linkman;
    private String phone;

    public FindWorks() {
    }

    public FindWorks(int riId, int cid, String job, String annual_salary, String location, String experience,
                     String education, String job_info, String release_time, String companyName, String industry,
                     String linkman, String phone) {
        this.riId = riId;
        this.cid = cid;
        this.job = job;
        this.annual_salary = annual_salary;
        this.location = location;
        this.experience = experience;
        this.education = education;
        this.job_info = job_info;
        this.release_time = release_time;
        this.companyName = companyName;
        this.industry = industry;
        this.linkman = linkman;
        this.phone = phone;
    }

    // Getter„ÄÅSetter


    public void setRiId(int riId) {
        this.riId = riId;
    }

    public void setCid(int cid) {
        this.cid = cid;
    }

    public void setJob(String job) {
        this.job = job;
    }

    public void setAnnual_salary(String annual_salary) {
        this.annual_salary = annual_salary;
    }

    public void setLocation(String location) {
        this.location = location;
    }

    public void setExperience(String experience) {
        this.experience = experience;
    }

    public void setEducation(String education) {
        this.education = education;
    }

    public void setJob_info(String job_info) {
        this.job_info = job_info;
    }

    public void setRelease_time(String release_time) {
        this.release_time = release_time;
    }

    public void setCompanyName(String companyName) {
        this.companyName = companyName;
    }

    public void setIndustry(String industry) {
        this.industry = industry;
    }

    public void setLinkman(String linkman) {
        this.linkman = linkman;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public int getRiId() {
        return riId;
    }

    public int getCid() {
        return cid;
    }

    public String getJob() {
        return job;
    }

    public String getAnnual_salary() {
        return annual_salary;
    }

    public String getLocation() {
        return location;
    }

    public String getExperience() {
        return experience;
    }

    public String getEducation() {
        return education;
    }

    public String getJob_info() {
        return job_info;
    }

    public String getRelease_time() {
        return release_time;
    }

    public String getCompanyName() {
        return companyName;
    }

    public String getIndustry() {
        return industry;
    }

    public String getLinkman() {
        return linkman;
    }

    public String getPhone() {
        return phone;
    }
}
