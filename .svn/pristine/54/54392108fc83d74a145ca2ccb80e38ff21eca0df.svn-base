.dropdown .dropdown-toggle .show-text {
  height: 100%;
  margin-right: 25px;
  vertical-align: middle;
}
.dropdown .dropdown-list {
  display: none;
  position: absolute;
  top: 100%;
  left: 0;
  min-width: 100%;
  max-height: 350px;
  overflow-y: auto;
  overflow-x: hidden;
  z-index: 999;
  border: 1px solid #ccc;
  background-color: #fff;
}
.dropdown:hover .dropdown-list {
  display: block;
  margin-left: -1px;
}
