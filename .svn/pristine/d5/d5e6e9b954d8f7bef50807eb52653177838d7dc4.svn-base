.hidden {
	display: none;
}

.wrapper_width {
	width: 400px;
}
.loginform_container {
	margin-top: 0;
    top: 50px;
    border: 1px solid #ddd;
    background-color: #fff;
    position: relative;
    max-width: 400px;
    padding: 0;
}
.login_container {
	overflow: hidden;
    padding: 30px;
    margin: 0 auto;
    text-align: center;
    background-color: aliceblue;
}
.input_group {
	overflow: hidden;
    background: #fff;
    position: relative;
    z-index: 1;
    display: inline-block;
    margin: 1em;
    max-width: 350px;
    width: calc(100% - 2em);
    vertical-align: top;
}
.imgVar_group {
	background: aliceblue;
    position: relative;
    z-index: 1;
    display: inline-block;
    margin: 1em;
    max-width: 350px;
    width: calc(100% - 2em);
    vertical-align: top;
}
.input_field {
	display: inline-block;
	padding: 12px 12px 12px 0px;
    width: 80%;
    background: transparent;
    float: right;
    border: none;
    font-size: 15px;
}
.input_label {
	float: left;
	display: inline-block;
    padding: 6px 0;
    cursor: default;
    text-align: center;
	width: 44px;
	height: 100%;
    font-size: 20px;
	background: #899dda;
}
.icon_hideo {
	color: #fff;
}

.form_actions {
	border-top: 1px solid #e4e4e4;
    background-color: #f7f7f7;
    padding: 15px 30px;
    text-align: right;
    font-size: 14px;
    line-height: 14px;
}
.form_actions>a:hover {
	color: #1a8fb4;
	text-decoration-line: none;
}
.btn_link {
	color: #aaa;
	padding: 10px 12px;
    margin-bottom: 0;
    font-size: 14px;
    line-height: 14px;
    vertical-align: middle;
}
.btn {
	display: inline-block;
    padding: 6px 12px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: normal;
    line-height: 14px;
    border: 1px solid transparent;
    border-radius: 4px;
    background-color: #1a8fb4;
    cursor: pointer;
}
.btn:hover {
	background-color: #00789d;
}